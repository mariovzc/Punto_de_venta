<%content_for :stylesheet do%>
<%end%>
<%= render 'menu/menu'%>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
  <section class="content-header">
    <ol class="breadcrumb">
      <li>
        <%= link_to "<i class='fa fa-user-plus'></i> Clientes".html_safe, :clients %>
      </li>
      <li class="active">Index</li>
    </ol>
  </section>
    <!-- Main content -->
  <section class="content">
  <% flash.each do |name, msg| %>
 <div class="alert alert-success alert-dismissable">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
     <h4>  <i class="icon fa fa-check"></i><%= msg %></h4>
  </div>
  <% end %>
  <%= link_to "<i class='fa fa-users'></i> Nuevo Cliente".html_safe, new_client_path, class: 'btn btn-app'%>
  <%= render 'clients'%>  
  </section><!-- /.content -->
</div><!-- /.content-wrapper -->
<%= render 'menu/footer'%>
<% content_for :javascript do %>
<%=javascript_include_tag "jquery.validate.min.js"%>
<%=javascript_include_tag "clients"%>
<%end%>
